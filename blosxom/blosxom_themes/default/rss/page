	<channel>
		<title>$blog_title</title>
		<link>$url</link>
		<description>$blog_description</description>
		<language>$blog_language</language>
		<lastBuildDate>$lastmodified::latest_rfc822</lastBuildDate>
		<generator>blosxom ver.$blosxom::version</generator>

<!-- blosxom date -->

<!-- blosxom story -->
		<item>
			<title>$title</title>
			<link>$url$path/$fn.$default_flavour</link>
			<description>$foreshortened::body</description>
			<guid isPermaLink="true">$url$path/$fn.$default_flavour</guid>
			<category>$path</category>
			<pubDate>$lastmodified::story_rfc822</pubDate>
			<content:encoded><![CDATA[$body]]></content:encoded>
			<comments>$url$path/$fn.$default_flavour#writeback</comments>
		</item>

<!-- blosxom foot -->
	</channel>
